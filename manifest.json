{
    "manifest_version": 3,
  
    "name": "tvprogram",
    "description": "TV program",
    "version": "1.5",

    "action": {
        "default_icon": { 
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
           "128": "icons/icon128.png"
        },
        "default_title": "Show tv program",
        "default_popup": "dist/index.html"
    },

    "host_permissions": [ 
        "https://tv.mail.ru/voronezh/channel/*"
    ]
}